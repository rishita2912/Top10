#include<stdio.h>
int main()
{
    int n,i;
    int arr[100];

    printf("Enter the number of elements in the array:");
    scanf("%d", &n);
    printf("Enter %d elements:",n);
    for(int i = 0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    //Reverse the array
    printf("Reversed array: ");
    for(i = n-1; i>=0; i--)
    {
        printf("%d\n", arr[i]);
    }
    printf("\n");
    return 0;
}
